<div class="well style-well">
  <div class="row">
    <div class="col-md-4">

    </div>
    <div class="col-md-4 well create-account">
      <h4>Create Account:</h4>
      <form action="/producer" method="post">
        <label for="producer[name]">Name:</label>
        <input type="text" name="producer[name]" class="form-control" required>
        <br>
        <label for="producer[username]">Username:</label>
        <input type="text" name="producer[username]" class="form-control" required>
        <br>
        <label for="producer[password]">Password:</label>
        <input type="text" name="producer[password]" class="form-control" required>
        <br>
        <button type="submit" class="btn btn-primary">Add User</button>
      </form>
    </div>
    <div class="col-md-4">

    </div>
  </div>
</div>
