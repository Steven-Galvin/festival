<h1>Hello <!-- user --> </h1>
<p>add artists to your schedule</p>
<form action="/attendee/:id" method="post">
  <label for="artist_id">Select Artist</label>
  <select id="artist_id" name="artist_id" type="text">
    <% @artists.each do |artist| %>
      <option value="<%= artist.id%>"> <%= artist.name %></option>
    <% end %>
  </select>
  <button class="btn btn-default" type="submit">Add artist</button>
</form>
